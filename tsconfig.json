{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "preserve",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "lib": ["ESNext", "DOM"],
    "skipLibCheck": true,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "types": ["vite-plugin-electron/electron-env"],
    "paths": {
      "@/*": ["./src/*"],
      "@unml/core": ["./packages/core/src"],
      "@unml/extensions": ["./packages/extensions/src"],
      "@unml/schema": ["./packages/schema/src"],
      "@unml/kit": ["./packages/kit/src"],
      "@unml/client": ["./packages/client/src"]
    }
  }
}
