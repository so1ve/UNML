{
	"name": "unml",
	"private": true,
	"version": "0.0.0",
	"packageManager": "pnpm@8.6.10",
	"author": "Ray <i@mk1.io> (https://github.com/so1ve)",
	"description": "UNML - UNlimited Minecraft Launcher",
	"license": "MIT",
	"main": "dist-electron/main.js",
	"scripts": {
		"build": "nr build:packages && nr build:app",
		"build:app": "nuxi build --prerender && electron-builder",
		"build:packages": "nr --filter \"./packages/*\" build",
		"dev": "nuxi dev",
		"postinstall": "nuxi prepare",
		"lint": "eslint . && prettier . --check",
		"lint:fix": "eslint . --fix && prettier . --write",
		"preview": "vite preview",
		"stub": "nr --filter \"./packages/*\" stub",
		"test": "vitest",
		"typecheck": "vue-tsc --noEmit"
	},
	"devDependencies": {
		"@iconify-json/material-symbols": "^1.1.52",
		"@iconify/vue": "^4.1.1",
		"@nuxt/devtools": "^0.7.1",
		"@nuxtjs/fontaine": "^0.4.1",
		"@nuxtjs/google-fonts": "^3.0.2",
		"@nuxtjs/i18n": "8.0.0-beta.13",
		"@pinia/nuxt": "^0.4.11",
		"@so1ve/eslint-config": "^0.117.2",
		"@so1ve/prettier-config": "^0.117.2",
		"@so1ve/utils": "^0.3.0",
		"@types/node": "^18.16.12",
		"@unocss/nuxt": "^0.54.0",
		"@unocss/preset-rem-to-px": "^0.54.0",
		"@vue-macros/nuxt": "^1.3.20",
		"@vueuse/nuxt": "^10.2.1",
		"crpr": "^0.2.0",
		"electron": "^25.3.2",
		"electron-better-ipc": "^2.0.1",
		"electron-builder": "^24.6.3",
		"electron-debug": "^3.2.0",
		"eslint": "^8.46.0",
		"nuxt": "^3.6.5",
		"nuxt-electron": "^0.6.0",
		"pathe": "^1.1.1",
		"pinia": "^2.1.6",
		"pkgroll": "^1.10.0",
		"prettier": "^3.0.0",
		"sass": "^1.64.1",
		"tsx": "^3.12.7",
		"typescript": "^5.1.6",
		"ufo": "^1.2.0",
		"unbuild": "^1.2.1",
		"vite-plugin-electron": "^0.12.0",
		"vite-plugin-electron-renderer": "^0.14.5",
		"vitest": "^0.33.0",
		"vue-tsc": "^1.8.8",
		"vuetify-nuxt-module": "^0.5.6"
	}
}
