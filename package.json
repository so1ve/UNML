{
  "name": "unml",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@8.1.0",
  "author": "Ray <i@mk1.io> (https://github.com/so1ve)",
  "description": "UNML - UNLimited Minecraft Launcher",
  "license": "MIT",
  "main": "dist-electron/main/index.js",
  "scripts": {
    "dev": "vite",
    "build:app": "nr typecheck && vite build && electron-builder",
    "build:packages": "rimraf packages/*/dist && nr --filter ./packages/* build",
    "build": "nr build:packages && nr build:app",
    "stub": "nr --filter ./packages/* stub && tsx scripts/fix-stub.ts",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.2",
    "@iconify-json/material-symbols": "^1.1.36",
    "@intlify/unplugin-vue-i18n": "^0.10.0",
    "@mdi/font": "^7.2.96",
    "@so1ve/eslint-config": "^0.56.1",
    "@unml/core": "workspace:^",
    "@unml/kit": "workspace:*",
    "@unocss/preset-rem-to-px": "^0.50.6",
    "@unocss/reset": "^0.50.6",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vueuse/core": "^9.13.0",
    "electron": "^23.2.0",
    "electron-builder": "^23.6.0",
    "eslint": "^8.37.0",
    "fast-glob": "^3.2.12",
    "pinia": "^2.0.33",
    "rimraf": "^3.0.2",
    "sass": "^1.60.0",
    "tsx": "^3.12.6",
    "typescript": "^5.0.2",
    "unbuild": "^1.1.2",
    "unocss": "^0.50.6",
    "unplugin-auto-import": "^0.15.2",
    "unplugin-vue-components": "^0.24.1",
    "unplugin-vue-macros": "^1.11.2",
    "vite": "^4.2.1",
    "vite-plugin-electron": "^0.11.1",
    "vite-plugin-electron-renderer": "^0.13.13",
    "vite-plugin-pages": "^0.29.0",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vite-plugin-vuetify": "^1.0.2",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.2.0",
    "vuetify": "^3.1.12"
  }
}
