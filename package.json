{
  "name": "unml",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@7.30.3",
  "author": "Ray <i@mk1.io> (https://github.com/so1ve)",
  "description": "UNML - UNLimited Minecraft Launcher",
  "license": "MIT",
  "main": "dist-electron/main/index.js",
  "scripts": {
    "dev": "vite",
    "build:app": "nr typecheck && vite build && electron-builder",
    "build": "rimraf packages/*/dist && nr --filter ./packages/* build",
    "stub": "nr --filter ./packages/* stub",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@antfu/ni": "^0.20.0",
    "@so1ve/eslint-config": "^0.53.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "electron": "^23.2.0",
    "electron-builder": "^23.6.0",
    "eslint": "^8.36.0",
    "pkgroll": "^1.9.0",
    "rimraf": "^3.0.2",
    "typescript": "^5.0.2",
    "unbuild": "^1.1.2",
    "vite": "^4.2.1",
    "vite-plugin-electron": "^0.11.1",
    "vite-plugin-electron-renderer": "^0.13.11",
    "vue": "^3.2.47",
    "vue-tsc": "^1.2.0"
  }
}
