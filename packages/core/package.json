{
  "name": "@unml/core",
  "version": "0.0.0",
  "author": "Ray <i@mk1.io> (https://github.com/so1ve)",
  "description": "UNML Core",
  "keywords": [
    "unml",
    "core",
    "unml-core"
  ],
  "type": "module",
  "homepage": "https://github.com/so1ve/unml/tree/main/packages/core#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/so1ve/unml.git",
    "directory": "/"
  },
  "bugs": {
    "url": "https://github.com/so1ve/unml/issues"
  },
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub"
  },
  "dependencies": {
    "@unml/kit": "workspace:*",
    "hookable": "^5.5.2"
  },
  "devDependencies": {
    "@unml/schema": "workspace:^"
  }
}
